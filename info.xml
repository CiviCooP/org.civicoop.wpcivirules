<?xml version="1.0"?>
<extension key="org.civicoop.wpcivirules" type="module">
  <file>wpcivirules</file>
  <name>CiviRules for WordPress</name>
  <description>CiviRules actions/triggers that interact with the underlying Wordpress installation.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Kevin Levie</author>
    <email>kevin@levity.nl</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/civicoop/org.civicoop.wpcivirules</url>
    <url desc="CiviRules Extension">https://github.com/civicoop/org.civicoop.civirules</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
    <url desc="Maintainer">https://levity.nl</url>
  </urls>
  <releaseDate>2016-11-16</releaseDate>
  <version>1.1.0</version>
  <develStage>beta</develStage>
  <compatibility>
    <ver>4.6</ver>
    <ver>4.7</ver>
  </compatibility>
  <requires>
    <ext>org.civicoop.civirules</ext>
    <ext>org.civicoop.civiruleslogger</ext>
  </requires>
  <comments/>
  <civix>
    <namespace>CRM/WPCiviRules</namespace>
    <format>22.05.2</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>mgd-php@1.0.0</mixin>
  </mixins>
</extension>
